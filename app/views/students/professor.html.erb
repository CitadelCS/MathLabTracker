<p id="notice"><%= notice %></p>

<h1>Professor Students</h1>

<table>
  <thead>
    <tr>
      <th>Cwid</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Teacher</th>
      <th>Visits</th>
      <th>Last Login</th>
      <th colspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.CWID %></td>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.teacher %></td>
        <td><%= student.count %></td>
        <td><%= student.login_date %></td>
        <td><%= link_to 'Show', student %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



<%= link_to 'Home Page', root_path %>
